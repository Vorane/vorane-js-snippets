{
  "name": "vorane-snippets",
  "displayName": "vorane-snippets",
  "description": "vorane javascript code snippets",
  "version": "0.0.7",
  "publisher": "VoraneStudios",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vorane/vorane-snippets"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "python",
        "path": "./snippets/python.code-snippets"
      }
    ]
  },
  "devDependencies": {
    "json-concat": "^0.0.1"
  },
  "scripts": {
    "concatjs": "json-concat snippets/modules/js/ snippets/javascript.code-snippets",
    "concatpy": "json-concat snippets/modules/py/ snippets/python.code-snippets"
  },
  "dependencies": {
    "generator-code": "^1.5.5"
  }
}
